{
"name": "citybuilder-3000",
"version": "1.0.0",
"scripts": {
    "dev:server": "cd server && npm run dev",
    "dev:client": "cd client && npm run dev",
    "install:all": "cd server && npm install && cd ../client && npm install",
    "start": "concurrently \"npm run dev:server\" \"npm run dev:client\""
},
"devDependencies": {
    "concurrently": "^8.2.2"
}
}
